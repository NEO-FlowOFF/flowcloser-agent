IQAI_API_KEY=your_iqai_api_key_here

# === LLM PRIORITY CHAIN ===
# Primary model (fallback handled via code)
LLM_MODEL=gpt-4o-mini
# Optional secondary (used only if primary fails)
LLM_MODEL_FALLBACK=gemini-2.5-flash

OPENAI_API_KEY=your_openai_api_key_here
# Google API Key (for Gemini models)
GOOGLE_API_KEY=your_google_api_key_here

# Server Port
PORT=8042

# Webhook Verify Token (para Instagram/WhatsApp)
WEBHOOK_VERIFY_TOKEN=flowcloser_webhook_neo

# Instagram OAuth (para autenticação)
INSTAGRAM_APP_ID=your_instagram_app_id
INSTAGRAM_APP_SECRET=your_instagram_app_secret
INSTAGRAM_REDIRECT_URI=https://flowcloser-agent-production.up.railway.app/api/auth/instagram/callback
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token
# INSTAGRAM_PAGE_ID=your_page_id (opcional, se necessário)

# WhatsApp Business API (Meta)
WHATSAPP_ACCESS_TOKEN=your_whatsapp_access_token
WHATSAPP_PHONE_NUMBER_ID=your_phone_number_id
WHATSAPP_BUSINESS_ACCOUNT_ID=your_business_account_id

# ----------------------------------------------------------------------------
# STORACHA (IPFS Pinning / Upload)
# ----------------------------------------------------------------------------
# UCAN/Proof de delegação (base64) com permissões: space/blob/add, space/index/add, filecoin/offer, upload/add
# Dica: remova todas as quebras de linha antes de colar.
STORACHA_UCAN=your_storacha_ucan_base64

# DID do espaço Storacha (opcional se o proof já embute o espaço, mas recomendado)
STORACHA_SPACE_DID=did:key:your_space_did

# (Opcional) DID do agente (apenas informativo / debug)
STORACHA_DID=did:key:your_agent_did

# Web3/Descentralizado (Opcional - para integrações futuras)
KWIL_PROVIDER=https://provider.kwil.com
CERAMIC_NODE=https://ceramic-clay.3boxlabs.com
PINATA_JWT=your_pinata_jwt
DID=did:key:your_did
TREASURY_WALLET=0x...

# Railway URL (opcional)
RAILWAY_URL=https://railway.com/project/your-project-id